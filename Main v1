local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/dadadadadadwadsa/bluhub-beta/refs/heads/main/ui%20lib"))() --you can go into the github link and copy all of it and modify it for yourself.
local Window = Library:CreateWindow("Flip's Gui v8", Vector2.new(492, 598), Enum.KeyCode.RightControl) --you can change your UI keybind
local AimingTab = Window:CreateTab("Universal") --you can rename this tab to whatever you want --you can also change the tabs code, for example "AimingTab" can be changed to "FunnyCoolTab" etc.
local play = game.Players.LocalPlayer
local char = play.Character
local hum = char.Humanoid

local testSection = AimingTab:CreateSector("Speed Stuff", "left")  --you can  change the section code, for example "testsection" can be changed to "FunnyCoolSection" etc.

testSection:AddButton("Speed Boost", function(IhateGayPeople)
    hum.WalkSpeed += 4
end)

testSection:AddSlider("Speed Slider", 0, 16, 100, 1, function(State)
    hum.WalkSpeed = State
end)

testSection:AddTextbox("Speed TextBox", nil, function(State)
    hum.WalkSpeed = State
end)

--testSection:AddDropdown("Dropdown", {"AH", "ytytyt", "rrro", "awhjdajksd"}, "AH", true, function(dropdown)

--end)

--local ColorToggle = testSection:AddToggle("ColorPicker w/Toggle", false, function(e)

--end)

local separator = testSection:AddSeperator("Jump Stuff")

testSection:AddButton("JumpPower Boost", function(IhateGayPeople)
    hum.JumpPower += 4
end)

testSection:AddSlider("JumpPower Slider", 0, 50, 100, 1, function(State)
    hum.JumpPower = State
end)

testSection:AddTextbox("JumpPower TextBox", nil, function(State)
    hum.JumpPower = State
end)

