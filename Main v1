local library = loadstring(game:GetObjects("rbxassetid://71920117413235")[1].Source)()
local Wait = library.subs.Wait

local plr = game.Players.LocalPlayer

local char = plr.Character or plr.CharacterAdded:Wait()

local hum = char.Humanoid

local Main = library:CreateWindow({
Name = "Flip's Gui",
Themeable = {
Info = "Discord Server: VzYTJ7Y"
}
})

local GeneralTab = Main:CreateTab({
   Name = "General"
})

local Section1 = GeneralTab:CreateSection({
  Name = "Section 1"
})
--Section1:AddToggle({
  -- Name = "Print Test",
  -- Flag = "PrintTest_Toggler",
  -- Callback = print
--})

Section1:AddSlider({
    Name = "Walkspeed",
    Flag = "Walk_Speed",
    Value = 16,
    Min = 0,
    Max = 200,
	Callback = function(Value)
			hum.WalkSpeed = Value
	end
})

Section1:AddButton({
    Name = "Reset WalkSpeed",
    Callback = function()
        print("Fixed WalkSpeed")
		hum.WalkSpeed = 16
    end
})


